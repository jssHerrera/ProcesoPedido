<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd"
    version="2.2">
 
 <navigation-rule> 
    
    <from-view-id>*</from-view-id> 
    
	<!-- Login -->
     
     <navigation-case> 
      <from-outcome>login</from-outcome> 
      <to-view-id>login.xhtml</to-view-id> 
    </navigation-case> 
    
    <!-- Panel -->
    
    <navigation-case> 
      <from-outcome>panel</from-outcome> 
      <to-view-id>inicio/panel.xhtml</to-view-id> 
    </navigation-case> 
    
    <!-- Productos -->
    
    <navigation-case> 
      <from-outcome>producto_listado</from-outcome> 
      <to-view-id>maestros/producto_listado.xhtml</to-view-id> 
    </navigation-case> 
    
     <navigation-case> 
      <from-outcome>producto_registro</from-outcome> 
      <to-view-id>maestros/producto_registro.xhtml</to-view-id> 
    </navigation-case> 
    
    <!-- Usuario -->
    
     <navigation-case> 
      <from-outcome>usuario_listado</from-outcome> 
      <to-view-id>seguridad/usuario_listado.xhtml</to-view-id> 
    </navigation-case> 
    
      <navigation-case> 
      <from-outcome>usuario_registro</from-outcome> 
      <to-view-id>seguridad/usuario_registro.xhtml</to-view-id> 
    </navigation-case> 

 </navigation-rule>

</faces-config>
